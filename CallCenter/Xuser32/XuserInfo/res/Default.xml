<?xml version="1.0" encoding="gb2312"?>

<skin xmlns="http://2451.vicp.net/schemas/Skin.xsd" version="1.0">

<!-- Toolbar Definitions -->
<toolbars>
	<!-- Agency WINDOW -->
	<toolbar name="CUserWnd.Header">
		<rightAlign/>
		<button id="ID_USER_TOPIC" text="个人信息管理" tip="点击选择分类"/>
		<separator />
		<label text="时间:" tip="当前系统时间"/>
		<control id="1001" width="142"/>
	</toolbar>
	<toolbar name="CUserWnd.Bottom">
		<button id="ID_MEDIA_PLAY" text="播放/编辑"/>
		<button id="ID_MEDIA_REMOVE" text="删除"/>
		<separator />
		<button id="ID_MEDIA_REFRESH" text="刷新" tip="重新下载语音记录。"/>
		<separator />
		<button id="ID_MEDIA_RENEW" text="标记为最新"/>
		<separator />
		<rightAlign/>
		<label text="号码+[分类]:" tip="在这里输入您要上传的电话号码，比如：960312345，其中5表示分类"/>
		<control id="1003" width="142"/>
		<button id="ID_SEARCH_QUICK" text="上传" tip="上传资料。"/>
	</toolbar>
</toolbars>

