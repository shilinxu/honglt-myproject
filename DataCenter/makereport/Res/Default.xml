<!-- Menu Definitions -->
<menus>
	<!-- MAIN WINDOW MENU BAR -->
	<menu name="CMainWnd" type="bar">
        <menu text="网络(_N)">
            <item id="ID_NETWORK_CONNECT" text="连接(_C)" shortcut="Ctrl+G"/>
            <item id="ID_NETWORK_DISCONNECT" text="断开连接(_D)"/>
            <separator/>
            <item id="ID_TAB_HOME" text="会话首页"/>
            <separator/>
            <!--item id="ID_TAB_MOBILE" text="中国移动(_M)"/-->
            <item id="ID_TAB_UNICOM" text="复盘文件(_U)"/>
            <!--item id="ID_TAB_TELECOM" text="中国电信(_T)"/-->
            <separator/>
            <item id="ID_APP_EXIT" text="退出(_X)"/>
        </menu>
		<menu text="报表(_R)">
			<item id="ID_TAB_REPORT" text="帐单明细(_R)" shortcut="Ctrl+R"/>
			<separator/>
			<item id="ID_REPORT_MUSIC" text="点歌统计(_M)..."/>
			<item id="ID_REPORT_PRINT" text="帐单打印(_P)..."/>
		</menu>
		<menu text="作业(_J)">
		</menu>
		<menu text="工具(_T)">
			<item id="ID_TOOLS_PROFILE" text="业务规则(_F)"/>
			<item id="ID_TOOLS_PRESSER" text="信箱规则(_M)"/>
			<separator/>
			<item id="ID_TOOLS_SETTINGS" text="系统设置(_S)..."/>
		</menu>
		<menu text="窗口(_W)">
			<item id="ID_WINDOW_TOOLBAR" text="显示工具栏(_T)"/>
			<item id="ID_WINDOW_STATUS_BAR" text="显示状态栏(_S)"/>
			<separator/>
			<item id="ID_WINDOW_DEBUG" text="显示调试信息(_D)"/>
		</menu>
		<menu text="帮助(_H)">
            <item id="ID_APP_ABOUT" text="关于(_A) 交换平台..."/>
		</menu>
	</menu>
</menus>

<!-- Toolbar Definitions -->
<toolbars>
	<!-- MAIN WINDOW -->
	<toolbar name="CMainWnd">
		<!--
        <button id="ID_TAB_CONNECT" text="连接"/>
		<separator />
		-->
        <button id="ID_TAB_HOME" text="首页"/>
		<separator />
        <!--button id="ID_TAB_MOBILE" text="中国移动"/-->
        <button id="ID_TAB_UNICOM" text="复盘文件"/>
        <!--button id="ID_TAB_TELECOM" text="中国电信"/-->
		<separator />
        <button id="ID_TOOLS_SETTINGS" text="系统设置"/>
		<separator />
        <button id="ID_APP_ABOUT" text="版本信息"/>
	</toolbar>
	<!-- UNICOM WINDOW -->
	<toolbar name="CUnicomWnd">
		<rightAlign/>
		<button id="ID_BROWSE_LAUNCH" text="预览..."/>
	</toolbar>
	<!-- TELECOM WINDOW -->
	<toolbar name="CTelecomWnd">
		<rightAlign/>
		<button id="ID_BROWSE_LAUNCH" text="预览..."/>
	</toolbar>
</toolbars>

